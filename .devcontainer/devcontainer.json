{
	"name": "Jekyll",
	"dockerFile": "Dockerfile",
	"settings": {
	  "terminal.integrated.defaultProfile.linux": "bash"
	},
	"forwardPorts": [4000],
	"postCreateCommand": "bundle install && bundle exec jekyll build",
	"extensions": [
	  "rebornix.Ruby",
	  "esbenp.prettier-vscode"
	],
	"runArgs": ["-u", "vscode"],
	"remoteUser": "vscode"
  }
  